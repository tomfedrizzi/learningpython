#Asignación de variables: 
"key" = "value"
variable = valor

Palabras reservadas en Py, no se pueden usar como variable.:
False - class - finally - is - return
None - continue - for - lambda - try
True - def - from - nonlocal - while
and - del - global - not - with
as - elif - if - or - yield
assert - else - import - pass - break
except - in - raise

num=int(input("Ingresá un número entero positivo: "))

print("A:")
print()
for i in range (1,num+1, 2):
    print(i, end=",")
    continue
print()
print()
print("B:")
print()
def cuenta_atras(b):
    while b >= 0:
        print(b, end=",")
        b -=1
        continue
cuenta_atras(num)

print()
print()
print("C:")
print()
def primos(d):
    contador=0    
    res=True
    for i in range(1,num+1):
        if (num%i==0):
            contador+=1
        if (contador>2):
            res=False
            break
    return res
if primos(num)==True:
    print(f"El '{num}' es primo.")
else:
    print(f"El '{num}' no es primo.")
primos(num)
print()
print("D:")

def factorial(a):
    fact_num = 1
    for i in range(1,a+1):  # de uno en uno en un rango desde 1 hasta el a(num q ingresa el usuario + 1)
        fact_num = fact_num * i #mu
    print(f"La factorial de {num} es {fact_num}") #agregar print a la misma escala q el for sino se printea muchas veces
    return fact_num
factorial(num)
